{
  "name": "memory-store",
  "version": "1.1.0",
  "description": "Gives Claude persistent memory across sessions. Records corrections with /correct, remembers patterns and architecture decisions, loads relevant context automatically. Claude stops forgetting and starts learning from every mistake.",
  "author": {
    "name": "Autotelic Team",
    "email": "developers@autotelic.inc"
  },
  "homepage": "https://github.com/julep-ai/memory-store-plugin",
  "repository": "https://github.com/julep-ai/memory-store-plugin",
  "license": "MIT",
  "keywords": [
    "memory",
    "tracking",
    "context",
    "development-flow",
    "team-collaboration",
    "git-analysis",
    "session-tracking",
    "knowledge-sharing"
  ],
  "mcpServers": {
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://beta.memory.store/mcp"
      ],
      "oauth": {
        "enabled": true,
        "discovery_url": "https://beta.memory.store/.well-known/oauth-protected-resource"
      }
    }
  }
}
